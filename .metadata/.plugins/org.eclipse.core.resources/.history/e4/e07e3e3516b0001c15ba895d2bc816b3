package br.com.luiz.imposto.entities;

public class Contribuinte {
	
	private String nome;
	private String rendaBruta;
	private String aliquota;
	private String parcelaDeduzir;
	
	public Contribuinte() {
	}
	
	public Contribuinte(String nome, String rendaBruta, String aliquota, String parcelaDeduzir) {
		this.nome = nome;
		this.rendaBruta = rendaBruta;
		this.aliquota = aliquota;
		this.parcelaDeduzir = parcelaDeduzir;
	}

	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	public String getRendaBruta() {
		return rendaBruta;
	}
	public void setRendaBruta(String rendaBruta) {
		this.rendaBruta = rendaBruta;
	}
	public String getAliquota() {
		return aliquota;
	}
	public void setAliquota(String aliquota) {
		this.aliquota = aliquota;
	}
	public String getParcelaDeduzir() {
		return parcelaDeduzir;
	}
	public void setParcelaDeduzir(String parcelaDeduzir) {
		this.parcelaDeduzir = parcelaDeduzir;
	}
	
	

}
